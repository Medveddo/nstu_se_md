bench_float_o0:
	bash -c "g++ -std=c++11 -O0 bench.cpp -o bench_float && ./bench_float float O0 fisqrt && rm ./bench_float"

bench_float_o1:
	bash -c "g++ -std=c++11 -O1 bench.cpp -o bench_float && ./bench_float float O1 fisqrt && rm ./bench_float"

bench_float_o2:
	bash -c "g++ -std=c++11 -O2 bench.cpp -o bench_float && ./bench_float float O2 fisqrt && rm ./bench_float"

bench_float_o3:
	bash -c "g++ -std=c++11 -O3 bench.cpp -o bench_float && ./bench_float float O3 fisqrt && rm ./bench_float"

bench_double_o0:
	bash -c "g++ -std=c++11 -O0 bench.cpp -o bench_double && ./bench_double double O0 fisqrt && rm ./bench_double"

bench_double_o1:
	bash -c "g++ -std=c++11 -O1 bench.cpp -o bench_double && ./bench_double double O1 fisqrt && rm ./bench_double"

bench_double_o2:
	bash -c "g++ -std=c++11 -O2 bench.cpp -o bench_double && ./bench_double double O2 fisqrt && rm ./bench_double"

bench_double_o3:
	bash -c "g++ -std=c++11 -O3 bench.cpp -o bench_double && ./bench_double double O3 fisqrt && rm ./bench_double"

bench_float_slow_o0:
	bash -c "g++ -std=c++11 -O0 bench.cpp -o bench_float && ./bench_float float O0 isqrt && rm ./bench_float"

bench_float_slow_o3:
	bash -c "g++ -std=c++11 -O3 bench.cpp -o bench_float && ./bench_float float O3 isqrt && rm ./bench_float"

bench_double_slow_o0:
	bash -c "g++ -std=c++11 -O0 bench.cpp -o bench_float && ./bench_float double O0 isqrt && rm ./bench_float"

bench_double_slow_o3:
	bash -c "g++ -std=c++11 -O3 bench.cpp -o bench_float && ./bench_float double O3 isqrt && rm ./bench_float"

